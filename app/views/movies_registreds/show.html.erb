
<h1>Filmes registrados</h1>
<%= link_to 'Home', root_path %><br>
<%= link_to 'Cadastre um novo filme', new_movie_path %><br>
<% @movies.each do |f| %>
    <%= link_to f.name, movie_path(f.id) %>
    <%= f.release_year %>
    <%= f.release_year > Time.current.year ? flash.notice = 'a ser lançado' : flash.notice = 'já lançado' %>
    <br>
<% end %>